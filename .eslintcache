mport ImageUpload from './ImageUpload.js';\n\nfunction getModalStyle() {\n  const top = 50;\n  const left = 50;\n\n  return {\n    top: `${top}%`,\n    left: `${left}%`,\n    transform: `translate(-${top}%, -${left}%)`,\n  };\n}\n\nconst useStyles = makeStyles((theme) => ({\n  paper: {\n    position: 'absolute',\n    width: 400,\n    backgroundColor: theme.palette.background.paper,\n    border: '2px solid #000',\n    boxShadow: theme.shadows[5],\n    padding: theme.spacing(2, 4, 3),\n  },\n}));\n\nfunction App() {\n  const classes = useStyles();\n  const [modalStyle] = useState(getModalStyle);\n  const [posts, setPosts] = useState([]);\n  const [open, setOpen] = useState(false);\n  const [openSignIn, setOpenSignIn] = useState(false);\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n  const [email, setEmail] = useState('');\n  const [user, setUser] = useState(null);\n\n  useEffect(()=> {\n    const unsubscribe = auth.onAuthStateChanged((authUser) => {\n      if (authUser) {\n        console.log(authUser);\n        setUser(authUser);\n\n      if (authUser.displayName) {\n      } else {\n        return authUser.updateProfile({\n          displayName: username,\n        });\n      }\n\n      } else {\n        setUser(null);\n      }\n    })\n\n    return () => {\n      unsubscribe();\n    }\n  }, [user, username]);\n\n  useEffect(() => {\n    db.collection('posts').onSnapshot(snapshot => {\n      setPosts(snapshot.docs.map(doc => ({\n        id: doc.id,\n        post: doc.data()\n      })));\n    })\n  }, []);\n\n  const signUp = (event) => {\n    event.preventDefault();\n\n    auth.createUserWithEmailAndPassword(email, password)\n    .then((authUser) => {\n      authUser.user.updateProfile({\n        displayName: username\n      })\n    })\n    setOpen(false);\n  };\n\n  const signIn = (event) => {\n    event.preventDefault();\n    auth.signInWithEmailAndPassword(email, password)\n    setOpenSignIn(false);\n  }\n\n  return (\n    <div className=\"App\">\n\n      {user?.displayName ? (\n      <ImageUpload username={user.displayName} />\n      ): (\n        <h3>Sorry you need to login to upload</h3>\n        )}\n\n      <Modal\n        open={open}\n        onClose={() => setOpen(false)}\n      >\n      <div style={modalStyle} className={classes.paper}>\n\n      <center>\n       <form className=\"app_signup\">\n        <img\n          className=\"app_headerImage\"\n          src=\"https://i.pinimg.com/originals/fc/26/50/fc26502254db62ce6b29debec1a56e80.png\"\n          alt=\"logo\"\n        />\n\n        <Input\n          placeholder=\"username\"\n          type=\"text\"\n          value={username}\n          onChange={(e) => setUsername(e.target.value)}\n        />\n        <Input\n          placeholder=\"email\"\n          type=\"text\"\n          value={email}\n          onChange={(e) => setEmail(e.target.value)}\n        />\n        <Input\n          placeholder=\"password\"\n          type=\"password\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n        />\n        <Button type=\"submit\" onClick={signUp}>Sign Up</Button>\n      </form>\n      </center>\n      </div>\n\n      </Modal>\n\n            <Modal\n        open={openSignIn}\n        onClose={() => setOpenSignIn(false)}\n      >\n      <div style={modalStyle} className={classes.paper}>\n\n      <center>\n       <form className=\"app_signup\">\n        <img\n          className=\"app_headerImage\"\n          src=\"https://i.pinimg.com/originals/fc/26/50/fc26502254db62ce6b29debec1a56e80.png\"\n          alt=\"logo\"\n        />\n\n        <Input\n          placeholder=\"username\"\n          type=\"text\"\n          value={username}\n          onChange={(e) => setUsername(e.target.value)}\n        />\n        <Input\n          placeholder=\"email\"\n          type=\"text\"\n          value={email}\n          onChange={(e) => setEmail(e.target.value)}\n        />\n        <Input\n          placeholder=\"password\"\n          type=\"password\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n        />\n        <Button type=\"submit\" onClick={signUp}>Sign Up</Button>\n      </form>\n      </center>\n      </div>\n\n      </Modal>\n\n      <div className=\"app_header\">\n        <img\n          className=\"app_headerImage\"\n          src=\"https://i.pinimg.com/originals/fc/26/50/fc26502254db62ce6b29debec1a56e80.png\"\n          alt=\"logo\"\n        />\n      </div>\n\n      { user ? (\n      <Button onClick={() =>auth.signOut()}>Logout</Button>\n      ): (\n      <div className=\"app_loginContainer\">\n        <Button onClick={() => setOpenSignIn(true)}>Sign in</Button>\n        <Button onClick={() => setOpen(true)}>Sign up</Button>\n      </div>\n\n      )}\n\n      <div className=\"app_posts\">\n        <div className=\"app_postsLeft\">\n      {\n        posts.map(({id, post}) => (\n          <Post key={id} caption={post.caption} username={post.username} imageUrl={post.imageUrl} />\n          ))\n      }\n        </div>\n      </div>\n\n\n\n\n    </div>\n  );\n}\n\nexport default App;\n","/Users/gvholley/code/gvholley/reactinsta/src/Post.js",[],"/Users/gvholley/code/gvholley/reactinsta/src/firebase.js",[],"/Users/gvholley/code/gvholley/reactinsta/src/index.js",[],"/Users/gvholley/code/gvholley/reactinsta/src/ImageUpload.js",[],{"ruleId":"29","severity":1,"message":"30","line":2,"column":8,"nodeType":"31","messageId":"32","endLine":2,"endColumn":12},{"ruleId":"29","severity":1,"message":"33","line":89,"column":9,"nodeType":"31","messageId":"32","endLine":89,"endColumn":15},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'signIn' is assigned a value but never used."]